#include <iostream>
#include <cmath>

using namespace std;

long superpower ( long a, long b){

  return pow(a,b);
}

int main (){

long power,a,b;
power = superpower (a,b);

  cout << "Numero 1: ";
  cin >> a;

  cout << "Numero 2: ";
  cin >> b;

  cout << "Resultado: "<< superpower(a,b)<< endl;


  return 0;
}
