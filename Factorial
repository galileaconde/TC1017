#include <iostream>
using namespace std;

long double factorial(int num)
{
	long double fact=1;

	for (int i = 1; i <= num; ++i)
	{
		fact=fact*i;
	}

return fact;
}

int main()
{
	int num;

	cout << "Give me a number: ";
	cin >> num;

	cout << "The factorial of " << num << " is: " << factorial(num) << endl;


return 0;
}
