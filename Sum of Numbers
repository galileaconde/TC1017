#include <iostream>

using namespace std;

int main (){

int a;
int b;
int sum = 0;

cout << "Please give us the lower bound: ";
  cin >> a;

  cout << "Please give us the upper bound: ";
    cin >> b;

  while (a <= b){
    sum = sum + a++;
  }

  cout << "The sum of the numbers is: " << sum << endl;
  return 0;
}
