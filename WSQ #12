#include <iostream>
using namespace std;

int mindiv( int a, int b )
{
   if( a < b )
      return mindiv( b, a );

   int f = a % b;
   if( f == 0 )
     return b;
   else
      return mindiv( b, f );
}

int main(){
  int a;
  int b;
  cout<<"Enter first number: "<<endl;
  cin>>a;
  cout<<"Enter second number: "<<endl;
  cin>>b;

  cout<<mindiv(a,b)<<endl;
return 0;
}
